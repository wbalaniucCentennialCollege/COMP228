
public class Account 
{
	private String name;
	
	// Allow the name of the account to be set
	public void setName(String name)
	{
		if(name == "Some really bad swear word")
		{
			this.name = "Fluffy bunnies";
		}
		else
		{
			this.name = name;
		}
	}
	
	public String getName()
	{
		return name;
	}
}
